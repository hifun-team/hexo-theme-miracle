
// new
.c-nav{
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    -webkit-transition-timing-function: cubic-bezier(0,0,.2,1);
    transition-timing-function: cubic-bezier(0,0,.2,1);
    -webkit-transition-duration: .3s;
    transition-duration: .3s;
    -webkit-transition-property: left,right;
    transition-property: left,right;
    will-change: left,right;
}
.c-nav{
    position fixed
    top 0
    background: rgba(253,253,253,0.5);
	backdrop-filter: blur(10px);
	-webkit-backdrop-filter: blur(10px);
	box-shadow: 0 0 8px rgba(0,0,0,0.25);
    z-index 9999
	// height 80px
}
.container{
    max-width 885px
    margin:0 auto;
}
.c-nav ul{
    line-height 30px
}
.c-nav li a:hover{
    color #3498db
}
.c-nav .show{
	display: inline-block;
}
.c-nav .hiden{
	display: none;
}
/* 导航栏 */
.c-nav{
    // background-color:#34495e;
	width: 100%;
}
.c-nav h1{
	font-size 1.7rem
}
.c-nav .logo{
    color:#4c4c4c
    padding-left: 20px!important;
    line-height 85%
    float left
    // text-transform uppercase
}
.c-nav .navFlex{
	display: flex;
	display: -webkit-flex;
	justify-content:space-between;
	-webkit-justify-content: space-between;
	align-items: center;
	-webkit-align-content: center;
	color:#4c4c4c;
    width: 100%;
}
.c-nav ul{
	list-style: none;
	margin-bottom: 0px;
	padding-left: 0px;
}
.c-nav ul li{
	padding: 15px 0px 15px 0px;
	margin-left: 30px;
	display: inline-block;
}
.c-nav ul li a{
	color: #4c4c4c;
	padding-bottom: 2px;
	text-decoration: none;
	border-bottom: 3px solid transparent;
}
.c-nav .logo{
	// height: 30px;
}
.c-nav .navFlex ul{
	// backdrop-filter: blur(10px);
    // -webkit-backdrop-filter: blur(10px);
}
.c-nav .btnImg{
    color: #4c4c4c;
	height: 20px;
	width: 25px;
	padding: 0px 8px 0px 8px;
	box-sizing: content-box;
	border: 1px solid transparent;
}

@media screen and (max-width: 885px){
	.c-nav{
		padding: 10px 0px 10px 0px;
		// height 60px
	}
	.c-nav .navFlex{
		flex-wrap: wrap;
		font-size: 1.7rem;
		justify-content: space-between;
	}
	.c-nav ul li{
		padding-top: 10px;
		margin-left: 0px;
		display: block;
	}
	.c-nav ul li a{
		border-bottom: 3px solid transparent;
	}
	.c-nav ul li:nth-child(4),.c-nav ul li:nth-child(5){
		display: block;
	}
	.c-nav .hiden{
		display: block;
	}
	.c-nav .show{
		width: 100%;
		font-size: 14px;
		text-align: center;
		display: none;
	}
}
@media screen and (min-width: 885px){
    .c-nav ul{
        float right 
    }
    .c-nav ul a{
        color #ffffff
        text-decoration none
        padding 0 10px
        transition 0.4s
    }
}