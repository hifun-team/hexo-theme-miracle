

<article class="page">
<div id="post-meta-m">
        <h3><%= page.title %></h3>
</div>
<div class="article-m">
    <div id="article" class="markdown-body">
        <%- page.content %>
    </div>
</div>
<% if(page.comment) { %>
    <!-- Comments -->
    <div class="comments">
        <% var type = '_partial/comments/' + theme.comment.type %>
        <%- partial(type) %>
    </div>
<% } %>
</article>


