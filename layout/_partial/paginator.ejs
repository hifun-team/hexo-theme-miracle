<% if (page.total > 1){ %>
    <nav aria-label="navigation">
      <span class="pagination" id="pagination">
        <%- paginator({
          prev_text: '<i class="fa fa-angle-left"></i>',
          next_text: '<i class="fa fa-angle-right"></i>',
          mid_size: 2,
          escape: false
        }) %>
      </span>
    </nav>
<% } %>