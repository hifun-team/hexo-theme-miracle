<script src="<%- theme.static_prefix.typed %>typed.min.js"></script>
<!-- Slogan 打字机效果 -->
<script>
    var typed = new Typed('#typed', {
        stringsElement: '#typed-strings',
        typeSpeed: <%= theme.index.slogan_typing.typeSpeed %>,
        showCursor: true,
        cursorChar: '<%= theme.index.slogan_typing.cursorChar %>',
        autoInsertCss: true,
    });
</script>
