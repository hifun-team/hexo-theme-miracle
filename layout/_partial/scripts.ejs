<script src="<%- theme.static_prefix.jquery %>jquery.min.js"></script>
<script src="<%- theme.static_prefix.md5 %>md5.min.js"></script>
<script>
    console.log('\n' + ' %c Powered by Hexo Theme Miracle ' + ' %c https://github.com/hifun-team/hexo-theme-miracle ' + '\n' + '\n', 'color: #fff; background: #4F9BFA; padding:5px 0;', 'background: #FFF; padding:5px 0;');
</script>
<!-- 引入 JS 文件 -->
<%- partial('_partial/plugins/analytics.ejs') %>
<%- js('/js/cookie.js') %>
<%- js('/js/main.js') %>
<%- js('/js/darkmode.js') %>
<% if (theme.search.enable){ %>
    <%- js('/js/search.js') %>
    <%- partial('_partial/plugins/search.ejs') %>
<% } %>
<% if (theme.lazyload.enable){ %>
    <%- js('/js/lazyload.js') %>
<% } %>
<% if (theme.post.code.highlight){ %>
    <link href="/css/themes/code.css" rel="stylesheet">  
    <script src="/js/hljs.js"></script>  
    <script >hljs.initHighlightingOnLoad();</script>
<% } %>
<% if(is_post() && theme.post.toc.enable && page.toc !== false) { %>
    <script src="<%- theme.static_prefix.tocbot %>tocbot.min.js"></script>
    <script>
        window.tocbot.init({
            // Where to render the table of contents.
            tocSelector: '.post-toc',
            // Where to grab the headings to build the table of contents.
            contentSelector: '#post-content',
            // Which headings to grab inside of the contentSelector element.
            headingSelector: '<%- theme.post.toc.headingSelector %>' || 'h1, h2, h3, h4, h5, h6',
            // For headings inside relative or absolute positioned containers within content.
            hasInnerContainers: true,
            linkClass: 'tocbot-link',
            activeLinkClass: 'tocbot-active-link',
            listClass: 'tocbot-list',
            isCollapsedClass: 'tocbot-is-collapsed',
            collapsibleClass: 'tocbot-is-collapsible',
            collapseDepth: <%- theme.post.toc.collapseDepth %> || 0,
            scrollSmooth    : true
        });
        $(".toc-btn").click(function(){
            $(".post-toc").toggleClass("display-inline")
        });
    </script>
<% } %>
<% if (theme.global.web_analytics.busuanzi.enable || theme.post.views.enable){ %>
    <script src="<%- theme.static_prefix.busuanzi %>busuanzi.pure.mini.js"></script>
<% } %>
