<div class="c-nav">
	<div class="container navFlex">
		<div class="flexItem">
      		<a href="<%- url_for() %>">
        		<h1 class="logo"><%= theme.global.nav_title || config.title %></h1>
      		</a>
		</div>
		
		<div class="flexItem hiden logo">
			<i class="fa fa-bars btnImg"></i>
		</div>
		<div class="flexItem show">
			<ul>
				<% for (var i in theme.navbar.menu){ %>
          			<li><a href="<%- url_for(theme.navbar.menu[i]) %>"><%-  __(i) %></a></li>
				<% } %>
				<% if (theme.search.enable){ %>
				<li><a href="javascript:;" id="search-btn"><i class="fas fa-search meta-icon"></i></a></li>
				<% } %>
			</ul>
		</div>
	</div>
</div>
