<%
var banner_img = page.banner_img
%>

<div id="post-meta-m">
    <%- partial('_partial/plugins/post-meta.ejs') %>
</div>
<div class="article-m">
    <div id="article">
        <!-- <%- partial('_partial/plugins/post-meta.ejs') %> -->
        <%- page.content %>
    </div>
</div>
<% if((page.comment || theme.post.comment) && (theme.comment.valine.appid || theme.comment.gitalk.clientID)) { %>
    <!-- Comments -->
    <div class="comments">
        <div id="miracle-comments">
        <% var type = '_partial/comments/' + theme.comment.type %>
        <%- partial(type) %>
        </div>
    </div>
<% } %>



