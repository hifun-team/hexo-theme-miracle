<%
var banner_img = page.banner_img
%>

<div id="post-meta-m">
    <%- partial('_partial/plugins/post-meta.ejs') %>
</div>
<div class="article-m">
    <div id="article">
        <%- page.content %>
    </div>
</div>
<% if(page.comment || theme.post.comment) { %>
    <!-- Comments -->
    <div class="comments">
        <% var type = '_partial/comments/' + theme.comment.type %>
        <%- partial(type) %>
    </div>
<% } %>


